{
  "name": "lemonbar",
  "version": "1.0.0",
  "description": "A custom bar inventory and cocktail app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sync-data": "tsx --env-file=.env scripts/sync-data.ts",
    "watch-data": "chokidar \"data/inventory.csv\" \"data/recipes.json\" -c \"npm run sync-data\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.6.0",
    "nuxt": "^3.11.2",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.11.0",
    "@types/node": "^20.11.30",
    "@vue/test-utils": "^2.4.4",
    "chokidar-cli": "^3.0.0",
    "happy-dom": "^20.0.0",
    "prettier": "^3.2.5",
    "pug": "^3.0.2",
    "sass": "^1.72.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^4.0.18"
  }
}
