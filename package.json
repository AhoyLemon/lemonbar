{
  "name": "booz",
  "version": "1.0.0",
  "description": "A custom bar inventory and cocktail app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "postgenerate": "cp -r public/* .output/public/",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "sync-data": "tsx --env-file=.env scripts/sync-data.ts",
    "watch-data": "chokidar \"data/bottles.csv\" \"data/drinks.json\" -c \"npm run sync-data\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "test:ci": "vitest run --reporter=dot",
    "ci": "npx nuxi prepare && npm run typecheck && npm run test:ci",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@nuxtjs/sitemap": "^7.6.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.6.0",
    "nuxt": "^4.3.1",
    "qrcode": "^1.5.4",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@types/qrcode": "^1.5.6",
    "@vue/test-utils": "^2.4.6",
    "chokidar-cli": "^3.0.0",
    "dotenv": "^17.3.1",
    "happy-dom": "^20.6.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.8.1",
    "pug": "^3.0.3",
    "sass": "^1.97.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
